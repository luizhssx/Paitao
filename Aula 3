print("Exercicio #1")
nome = input("Digite seu nome aqui: ")
idade = int(input("Digite sua idade aqui:"))
"""input sempre retorna string"""
anoAtual = 2019
anoNascimento = anoAtual - idade
calculoIdade = anoNascimento + 100
print(nome + ", voce ir√° fazer sem anos em ", calculoIdade)


"""Aula 3 Exercicio 2"""
print("Exercicio 2")
nome = input("Digite seu nome aqui: ")
idade = int(input("Digite sua idade aqui:"))
qtd = int(input("Quantas vezes gostaria de imprimir esta mensagem? "))
anoAtual = 2019
anoNascimento = anoAtual - idade
calculoIdade = anoNascimento + 100
while (qtd !=0):
  print (nome+", voce ira fazer 100 anos em", calculoIdade)
  qtd = qtd -1
  
  print("Exercicio 3")
